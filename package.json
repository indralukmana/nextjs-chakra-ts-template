{
	"name": "pmp-fe",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"cypress:open": "cypress open",
		"dev": " cross-env NODE_OPTIONS='--inspect' NODE_ENV='development' next dev",
		"dev:docker": "docker-compose -f docker-compose.dev.yml up  --build --force-recreate",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"lint": "next lint",
		"lint:fix": "eslint src --fix && pnpm run format",
		"lint:strict": "eslint --max-warnings=0 --ext js,tsx,mdx,md src ",
		"prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
		"prettier": "prettier --write .",
		"prod:up": "docker-compose up --build --force-recreate",
		"start": "next start",
		"test": "jest",
		"test:watch": "jest --watch",
		"typecheck": "tsc --noEmit --incremental false"
	},
